<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gdufs.edu.model">
	<class name="Question" table="question">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="userId" column="userId" type="int"/>
		<property name="nickname" column="nickname" type="java.lang.String" />
		<property name="content" column="content" type="java.lang.String" />
		<property name="timestamp" column="timestamp" type="java.lang.String" />
		<property name="lastretime" column="lastretime" type="java.lang.String" />
		<property name="file" column="file" type="java.lang.String" />
		<property name="fileType" column="fileType" type="java.lang.String" />
		<property name="recount" column="recount" type="int" />
	</class>
</hibernate-mapping>